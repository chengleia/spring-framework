<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


	<bean id="helloWordTwo" class="org.springframework.study.HelloWordTwo"/>
	<bean id="helloWorld" name="q" class="org.springframework.study.HelloWorld" depends-on="helloWordTwo">
		<property name="helloWorld" ref="helloWorld"></property>
	</bean>

	<bean id="cl" class="org.springframework.study.TestFactory" factory-method="get"  scope="prototype" ></bean>

	<!--	<bean id="fb" class="org.springframework.study.TestFactoryBean" scope="prototype"></bean>-->

</beans>